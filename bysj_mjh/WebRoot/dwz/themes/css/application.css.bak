.jbtn.green { background: #9d4; }
.jbtn.pink { background: #e1a; }
.jbtn:hover { background-color: rgb(101, 57, 241); text-decoration: none;}
.jbtn:active { background-color: #576a73; }
.jbtn[class] {  background-image: url(../button3/shade.png); background-position: bottom; }


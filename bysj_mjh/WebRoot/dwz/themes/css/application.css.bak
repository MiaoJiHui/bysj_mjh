#sidebar .accordionContent { position:relative; }

#sidebar .menu li { text-align:center; }

#sidebar .menu li a.navTab { border-bottom:1px solid #fff; display:block; padding:5px; }

#sidebar .menu li a.navTab.selected { background:#efefef; border-bottom:1px solid #76AF31; }

#sidebar .menu li a.navTab:hover { text-decoration:none; }

#sidebar .menu li a.navTab:hover:not(.selected) { border-bottom:1px solid #76AF31; }

#sidebar .menu li a.navTab em { width:80px; display:inline-block; font-style:inherit; line-height:18px; }

#sidebar .menu li a.navTab span { display:inline-block; padding-left:16px; margin:0 auto 5px auto; }

#sidebar .menu li a.navTab span i { display:inline-block; float:left; font-style:inherit; width:12px; color:#ff5500; font-weight:700; white-space:nowrap; }

#sidebar .menu li a.navTab span img { float:left; width:32px; height:32px; }

#sidebar .btn { position:absolute; text-align:center; height:16px; background:#EAF4FF url(/content/images/sidebar/updown.png) no-repeat; cursor:pointer; width:100%; filter:alpha(Opacity=80); -moz-opacity:0; opacity:0; }

#sidebar .btn.up { background-position:center 0; top:0; border-bottom:1px solid #B8D0D6; }

#sidebar .btn.down { background-position:center -16px; bottom:0px; border-top:1px solid #B8D0D6; }